{
  "AllowedHosts": "*",
  "RSS": {
    "HubUrl": "http://rsshub:1200",
    "HubUrlOversea": ""
  },
  "OneBot": {
    "Url": "http://napcat:3000"
  },
  "OAuth": {
    "Github": {
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "DB": {
    "Path": "./data/notify.db"
  },
  "Auth": {
    "Admin": [
      "watora"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}